@model Engineer

<h2>Mühendis Ekle</h2>
<hr>
<form asp-controller="Home" asp-action="EngineerCreate" method="post">
    <div class="mb-3">
        <label asp-for="EngineerName" class="form-label">Mühendis Ad</label>
        <input asp-for="EngineerName" class="form-control">
    </div>
    <div class="mb-3">
        <label asp-for="EngineerUsername" class="form-label">Mühendis Soyad</label>
        <input asp-for="EngineerUsername" class="form-control">
    </div>
    <div class="mb-3">
        <label asp-for="EngineerEmail" class="form-label">Email</label>
        <input asp-for="EngineerEmail" class="form-control">
    </div>
    <div class="mb-3">
        <label asp-for="EngineerPhone" class="form-label">Telefon</label>
        <input asp-for="EngineerPhone" class="form-control">
    </div>
    <div class="mb-3">
        <label asp-for="JobDateTime" class="form-label">Staj başlangıç tarihi</label>
        <input asp-for="JobDateTime" class="form-control">
    </div>
    <button type="submit" class="btn btn-secondary">Kaydet</button>
</form>